{
  "name": "Skulptor Resource Manager",
  "version": "1.0",
  "description": "Page Action to manage files on Code Skulptor",
  "permissions": ["tabs","storage"],
  "background": { "scripts": ["js/background.js"] },
  "page_action": {
    "default_icon": "icon1.png", // optional
    "default_title": "Code Skulport Extension",    // optional; shown in tooltip
    "default_popup": "popup.html"    // optional
   },
   "content_scripts": [
    {
      "matches": ["*://www.codeskulptor.org/*"],
	    "css" : ["css/style.css"],
      "js": ["js/jquery-1.8.2.min.js", "js/content_script.js"]
    }
   ],
   "manifest_version": 2,
   "content_security_policy": " script-src 'self'; object-src 'self' "
}
